# 回滚生成行外，d2需占用1000默认值内容，d1是变成行外
begin;
update t_big_default set d2 = repeat('c',8600) where c1 = 11;
rollback;

alter table t_big_default add column d3 varchar(10000) not null default '';

begin;
update t_big_default set d3 = repeat('c',8600) where c1 = 12;
rollback;

select c1,c2,length(d1),length(d2),length(d3) from t_big_default;

checksum table t_big_default;

drop table t_big_default;

